<?xml version="1.0" encoding="UTF-8"?>

<!-- xml 기본 문법 , 스프링 설정 파일 -->
<beans
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans">

	<bean id="rectangleBean" class="rectangle.exam.RectangleImpl">
		<constructor-arg>
			<value>10.0</value>
		</constructor-arg>
		<constructor-arg>
			<value>4.0</value>
		</constructor-arg>
	</bean>
	
	<bean id="sadariBean" class="rectangle.exam.Sadari">
		<property name="base_lower">
			<value>20</value>
		</property>
		<property name="base_upper">
			<value>10</value>
		</property>
		<property name="height">
			<value>15</value>
		</property>
	</bean>
</beans>